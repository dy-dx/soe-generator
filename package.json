{
  "name": "soe-generator",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "budo src/app.js:bundle.js --live -v --dir static -- -t [ babelify --presets [ airbnb ] ]",
    "build": "browserify src/app.js -t [ babelify --presets [ airbnb ] ] > static/bundle.js",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint src --ext js --ext jsx",
    "stylelint": "stylelint static/css/**/*.css --syntax scss",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dy-dx/soe-generator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dy-dx/soe-generator/issues"
  },
  "homepage": "https://github.com/dy-dx/soe-generator#readme",
  "dependencies": {
    "airbnb-browser-shims": "^2.0.0",
    "babel-preset-airbnb": "^2.4.0",
    "babelify": "^7.3.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "budo": "^10.0.4",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0"
  }
}
